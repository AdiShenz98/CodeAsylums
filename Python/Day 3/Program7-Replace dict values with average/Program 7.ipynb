   "source": [
    "mydict={\"a1\":2,\"a2\":3,\"a4\":4,\"a5\":6}\n",
    "sum=0\n",
    "mylist=list(mydict.values())\n",
    "for i in mylist:\n",
    "    sum=sum+i\n",
    "avg=sum/(len(mydict))\n",
    "mydict={x:avg for x in mydict.keys()}"
   ]
